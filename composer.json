{
    "name": "chubbyphp/chubbyphp-api-http",
    "description": "Chubbyphp Api Http",
    "keywords": ["chubbyphp", "slim", "api", "http"],
    "license": "MIT",
    "authors": [
        {
            "name": "Dominik Zogg",
            "email": "dominik.zogg@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "chubbyphp/chubbyphp-deserialization": "^2.0",
        "chubbyphp/chubbyphp-negotiation": "^1.0",
        "chubbyphp/chubbyphp-serialization": "^2.0",
        "psr/http-factory": "^1.0.1",
        "psr/http-message": "^1.0.1",
        "psr/http-server-middleware": "^1.0.1"
    },
    "require-dev": {
        "chubbyphp/chubbyphp-mock": "^1.4",
        "infection/infection": "^0.12.2",
        "phploc/phploc": "^4.0|^5.0",
        "phpstan/phpstan": "^0.11.5",
        "phpunit/phpunit": "^7.0|^8.0",
        "pimple/pimple": "^3.1"
    },
    "autoload": {
        "psr-4": { "Chubbyphp\\ApiHttp\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Chubbyphp\\Tests\\ApiHttp\\": "tests/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.1-dev"
        }
    }
}
