{
    "name": "chubbyphp/chubbyphp-api-http",
    "description": "Chubbyphp Api Http",
    "keywords": ["chubbyphp", "slim", "api", "http"],
    "license": "MIT",
    "authors": [
        {
            "name": "Dominik Zogg",
            "email": "dominik.zogg@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "chubbyphp/chubbyphp-deserialization": "^2.1",
        "chubbyphp/chubbyphp-serialization": "^2.1",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "chubbyphp/chubbyphp-mock": "^1.4",
        "phploc/phploc": "^4.0|^5.0",
        "phpunit/phpunit": "^6.5|^7.0|^8.0",
        "pimple/pimple": "^3.1"
    },
    "autoload": {
        "psr-4": { "Chubbyphp\\ApiHttp\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Chubbyphp\\Tests\\ApiHttp\\": "tests/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev"
        }
    }
}
